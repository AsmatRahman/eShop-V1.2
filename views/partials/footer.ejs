<script>
   console.log(document.cookie)
   try {
     let userData = JSON.parse(decodeURIComponent(document.cookie.split('=')[1]));
     if(userData.user_name) {
       $('.not-logged-in').addClass('d-none');
       $('.logged-in').removeClass('d-none');
       $('.logged-in .user-name').text(userData.user_name);
     }
     else {
       $('.not-logged-in').removeClass('d-none');
       $('.logged-in').addClass('d-none');
       $('.logged-in .user-name').text('');
     }
   } catch (error) {
     $('.not-logged-in').removeClass('d-none');
     $('.logged-in').addClass('d-none');
     $('.logged-in .user-name').text('');
   }
   </script>
